{
  "name": "js-webpack-boilerplate",
  "version": "1.0.0",
  "description": "A simple JavaScript webpack boilerplate for modern web development",
  "main": "index.js",
  "scripts": {
    "start": "webpack serve --config webpack.dev.js",
    "build": "webpack --config webpack.prod.js",
    "watch": "webpack --watch --config webpack.dev.js",
    "lint": "eslint \"src/**/*.js\"",
    "lint:fix": "eslint \"src/**/*.js\" --fix",
    "format": "prettier --write \"src/**/*.{js,css,html}\"",
    "deploy": "gh-pages -d dist"
  },
  "keywords": [
    "webpack",
    "javascript",
    "boilerplate",
    "template"
  ],
  "author": "Binit Gupta",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.28.0",
    "@babel/preset-env": "^7.28.0",
    "babel-loader": "^10.0.0",
    "css-loader": "^7.1.2",
    "eslint": "^9.31.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.3",
    "html-webpack-plugin": "^5.6.3",
    "prettier": "^3.6.2",
    "style-loader": "^4.0.0",
    "webpack": "^5.100.2",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.2",
    "webpack-merge": "^6.0.1"
  }
}
